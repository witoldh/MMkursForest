<!DOCTYPE html>
<html lang="pl">

<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Kontakt</title>
    <!-- <link rel="stylesheet" href="./dist/css/hamburger.css"> -->
    <link rel="stylesheet" href="./dist/css/style.min.css">
</head>

<body>

    <!-- @include 'navigation.kit' -->

    <section class="row row--aboutUs">
        <div class="row__header">
            <h1 class="row__title">Forest Group</h1>
            <h2 class="row__subtitle">Skontaktuj się z nami</h2>
        </div>
    </section>
    
    <div class="contactContainer">
    <form class="contact">
        <label for "name" class="contact__label">Imię</label>      
        <input name="name" label="Imię" type="text" class="contact__input" placeholder="Imię /  nazwa firmy" />   
        <label for "email" class="contact__label">Adres e-mail</label>
        <input name="email" type="email" class="contact__input" placeholder="adres e-mail" />
        <label for "text" class="contact__label">Wiadomość</label>
        <textarea name="text" class="contact__input" placeholder="wpisz treść swojej wiadomości"></textarea>
        <button type="submit" class="contact__btn" >Wyślij</button>
      </form>
      <div class="googlemaps">
        <iframe class="googlemaps" src="https://www.google.com/maps/embed?pb=!1m18!1m12!1m3!1d2559.8980500890234!2d19.89026927751004!3d50.08819561337936!2m3!1f0!2f0!3f0!3m2!1i1024!2i768!4f13.1!3m3!1m2!1s0x47165a326d015c3b%3A0x47a12cd367a1f34e!2sRestauracja%20McDonald&#39;s!5e0!3m2!1spl!2spl!4v1706884098720!5m2!1spl!2spl" loading="lazy" referrerpolicy="no-referrer-when-downgrade"></iframe>
      </div>
    </div>
    

 
  



    <!-- @include 'footer.kit'-->

    <script src="./src/js/main.js"> </script>
</body>

</html>